{
  "name": "n8n-nodes-sharepoint",
  "version": "0.3.2",
  "description": "N8N Sharepoint node",
  "keywords": [
    "n8n-community-node-package"
  ],
  "license": "MIT",
  "homepage": "",
  "author": {
    "name": "Tim Smart",
    "email": "hello@timsmart.co"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/arisechurch/n8n-nodes-sharepoint.git"
  },
  "main": "index.js",
  "scripts": {
    "build": "rm -rf dist *.tsbuildinfo && tsc && gulp build:icons",
    "dev": "tsc --watch",
    "format": "prettier nodes credentials --write",
    "prepublishOnly": "npm run build"
  },
  "files": [
    "dist"
  ],
  "n8n": {
    "n8nNodesApiVersion": 1,
    "credentials": [
      "dist/credentials/MicrosoftSharepointApi.credentials.js"
    ],
    "nodes": [
      "dist/nodes/MicrosoftSharepoint/MicrosoftSharepoint.node.js"
    ]
  },
  "devDependencies": {
    "@tsplus-types/effect__data": "0.12.2-3b576ba",
    "@tsplus-types/effect__io": "0.22.5-3b576ba",
    "@types/request": "^2.48.8",
    "gulp": "^4.0.2",
    "n8n-core": "*",
    "n8n-workflow": "*",
    "prettier": "^2.8.7",
    "typescript": "https://cdn.jsdelivr.net/npm/@tsplus/installer@0.0.171/compiler/typescript.tgz"
  },
  "dependencies": {
    "@effect/data": "^0.12.2",
    "@effect/io": "^0.22.5",
    "@effect/match": "^0.15.0"
  }
}
